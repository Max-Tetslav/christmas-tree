{
	"plugins": [
		"prettier",
		"import"
	],
	"extends": [
		"airbnb-base",
    "airbnb-typescript/base",
		"plugin:prettier/recommended",
		"prettier"
	],
	"parserOptions": {
    "project": ["tsconfig.json"],
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"no-debugger": "off",
		"no-console": 0,
    "no-param-reassign": 0,
		"class-methods-use-this": "off",
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		]
	}
}